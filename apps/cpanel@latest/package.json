{
  "name": "cpanel",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production bun scripts/build.ts",
    "dev": "bun scripts/build.ts --watch",
    "lint": "bun run lint:format && bun run lint:types",
    "lint:format": "bun biome check --write",
    "lint:types": "bun tsc --noEmit",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@buntime/piercing": "workspace:*",
    "@buntime/plugin-authn": "workspace:*",
    "@buntime/plugin-authz": "workspace:*",
    "@buntime/plugin-durable": "workspace:*",
    "@buntime/plugin-gateway": "workspace:*",
    "@buntime/plugin-keyval": "workspace:*",
    "@buntime/plugin-metrics": "workspace:*",
    "@buntime/plugin-proxy": "workspace:*",
    "@buntime/runtime": "workspace:*",
    "@buntime/shadcn-ui": "workspace:*",
    "@buntime/shared": "workspace:*",
    "@iconify-icon/react": "^3.0.3",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.134.15",
    "@tanstack/react-table": "^8.21.3",
    "cmdk": "^1.1.1",
    "hono": "^4.11.0",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.0",
    "sonner": "^2.0.7",
    "tw-animate-css": "^1.4.0",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@zomme/bun-plugin-i18next": "^0.1.1",
    "@zomme/bun-plugin-iconify": "^0.1.2",
    "@zomme/bun-plugin-tsr": "^0.1.1",
    "bun-plugin-tailwind": "^0.1.2",
    "tailwindcss": "^4.1.18"
  },
  "buntime": {
    "entrypoint": "dist/index.js",
    "visibility": "protected",
    "publicRoutes": {
      "GET": [
        "/*.js",
        "/*.css",
        "/*.woff2",
        "/*.png",
        "/*.svg",
        "/*.ico"
      ]
    }
  }
}
