{
  "name": "@buntime/plugin-proxy",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./plugin.ts",
      "default": "./dist/plugin.js"
    }
  },
  "scripts": {
    "build": "bun scripts/build.ts",
    "dev": "bun scripts/build.ts --watch",
    "lint": "bun run lint:format && bun run lint:types",
    "lint:format": "bun biome check --write",
    "lint:types": "bun tsc --noEmit",
    "test": "bun test",
    "test:coverage": "bun test --coverage 2>&1 | grep -vE '\\.\\./(packages|plugins)'",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@buntime/plugin-keyval": "file:../plugin-keyval",
    "@buntime/shared": "file:../../packages/shared",
    "@iconify-icon/react": "^2.1.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tanstack/react-query": "^5.62.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.0.2",
    "tw-animate-css": "^1.3.4",
    "vaul": "^1.1.2"
  },
  "peerDependencies": {
    "hono": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "bun-plugin-tailwind": "^0.1.2",
    "tailwindcss": "^4.1.17"
  },
  "buntime": {
    "entrypoint": "index.ts"
  }
}
