{
  "name": "@buntime/plugin-authn",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./plugin.ts",
      "default": "./dist/plugin.js"
    }
  },
  "scripts": {
    "build": "echo 'Skipping disabled plugin'",
    "dev": "bun scripts/build.ts --watch",
    "lint": "bun run lint:format && bun run lint:types",
    "lint:format": "bun biome check --write",
    "lint:types": "bun tsc --noEmit",
    "start": "bun run build && bun --port=5010 dist/index.js",
    "test": "bun test",
    "test:coverage": "bun test --coverage 2>&1 | grep -vE '\\.\\./(packages|plugins)'",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@buntime/plugin-database": "workspace:*",
    "@buntime/shared": "workspace:*",
    "@libsql/client": "^0.15.0",
    "better-auth": "^1.4.6",
    "drizzle-orm": "^0.38.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "peerDependencies": {
    "hono": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@zomme/bun-plugin-tsr": "^0.1.1",
    "bun-plugin-tailwind": "^0.1.2",
    "tailwindcss": "^4.1.17"
  },
  "buntime": {
    "entrypoint": "dist/index.js"
  }
}
