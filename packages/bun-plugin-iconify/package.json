{
  "name": "@zomme/bun-plugin-iconify",
  "version": "0.1.2",
  "description": "Bun plugin that collects icons from source code and generates a virtual module with icon data from Iconify",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "LICENSE",
    "src"
  ],
  "keywords": [
    "bun",
    "bun-plugin",
    "iconify",
    "icons",
    "virtual-module"
  ],
  "author": "Djalma Junior <djalmajunior@gmail.com>",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "lint:format": "bun biome check --write",
    "lint:types": "bun tsc --noEmit",
    "lint": "bun run lint:format && bun run lint:types",
    "publish:npm": "npm publish --access public --otp="
  },
  "peerDependencies": {
    "@iconify-json/*": ">=1.0.0",
    "@iconify/json": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "@iconify-json/*": {
      "optional": true
    },
    "@iconify/json": {
      "optional": true
    }
  }
}
